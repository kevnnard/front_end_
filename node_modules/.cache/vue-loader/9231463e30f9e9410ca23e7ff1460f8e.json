ion(data) {\r\n\t\t\talert(\"Registro Exitoso\");\r\n\t\t\tthis.completedLogIn(data);\r\n    },\r\n\r\n    logOut: function () {\r\n\t\t\tlocalStorage.clear();\r\n\t\t\talert(\"Sesión Cerrada\");\r\n\t\t\tthis.verifyAuth();\r\n\t\t},\r\n  },\r\n\r\n  created: function(){\r\n    this.verifyAuth()\r\n  }\r\n\r\n}\r\n</script>\r\n\r\n\r\n\r\n\r\n\r\n\r\n<style>\r\n\r\n  body{\r\n    margin: 0 0 0 0;\r\n  }\r\n  .nav-new {\r\n    display: flex;\r\n    justify-content: center;\r\n  }\r\n  .main-component{\r\n    margin: 0%;\r\n    padding: 0%;\r\n    background: #FDFEFE ;\r\n  }\r\n  .logo-img {\r\n    width: 15%;\r\n  }\r\n  \r\n  #navbarNavDropdown {\r\n    justify-content: center;\r\n  }\r\n  \r\n\r\n  /* FOOTER */ \r\n\r\n\r\n</style>\r\n"]}]}