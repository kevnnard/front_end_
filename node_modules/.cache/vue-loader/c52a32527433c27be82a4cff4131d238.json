d\">\r\n    <div class=\"sub-menu\">\r\n        <ul>\r\n            <li><a class=\"btn btn-dark\" href=\"#\">Usuarios</a></li>\r\n            <li><a class=\"btn btn-dark\" href=\"#\">Centros</a></li>\r\n            <li><a class=\"btn btn-dark\" href=\"#\">Solicitudes</a></li>\r\n        </ul>\r\n    </div>\r\n    <div class=\"container\" >\r\n        <table class=\"table table-hover\" >\r\n        <thead>\r\n            <tr>\r\n                <th scope=\"col\">id</th>\r\n                <th scope=\"col\">Cédula</th>\r\n                <th scope=\"col\">Nombre</th>\r\n                <th scope=\"col\">Ciudad</th>\r\n                <th scope=\"col\">Estado Solicitud</th>\r\n                <th scope=\"col\">Centro</th>\r\n                <th scope=\"col\">Mensaje</th>\r\n            </tr>\r\n        </thead>\r\n        <tbody>\r\n            <tr v-for=\"solicitud in solicitudes\" v-bind:key=\"solicitud\">\r\n                <th scope=\"row\">{{solicitud.idSolicitud}}</th>\r\n                <td>{{solicitud.cedula}}</td>\r\n                <td>{{solicitud.nombreCliente}}</td>\r\n                <td>{{solicitud.ciudad}}</td>\r\n                <td>{{solicitud.FinalizedState}}</td>\r\n                <td>{{solicitud.Centro.idCentro}}</td>\r\n                <td>{{solicitud.Mensaje}}</td>\r\n            </tr>\r\n        </tbody>\r\n        </table>\r\n    </div>\r\n    </div>\r\n</template>\r\n\r\n\r\n<script>   \r\nimport axios from 'axios';\r\nimport funtion from 'funtion.js';\r\n\r\n\r\n\r\nexport default {\r\n    name: \"UserHome\",\r\n\r\n    data: function(){\r\n        return {\r\n            solicitud: [],\r\n            solicitudes: []\r\n        }\r\n    },\r\n\r\n    methods: {\r\n        getSolicitudesList: async function(){\r\n            axios.get(\r\n                \"https://misiontic-2021-p12-g2.herokuapp.com/solicitud/get/\",\r\n                {headers: {}}\r\n            )\r\n            .then((result) =>{\r\n                this.solicitudes = result.data;\r\n            })\r\n            .catch((error) => {\r\n                console.log(error)\r\n                alert(\"Error obteniendo lista de solicitudes\");\r\n            })\r\n        }\r\n    },\r\n    created: async function(){\r\n        this.getSolicitudesList();\r\n    }\r\n}\r\n\r\n\r\n</script>\r\n\r\n\r\n<style>\r\n    *, ul , li {\r\n        list-style: none;\r\n    }\r\n    ul li > a {\r\n        text-decoration: none;\r\n        color: #000;\r\n    }\r\n    ul {\r\n        display: flex;\r\n        justify-content: center;\r\n        gap: 15px;\r\n    }\r\n    .greetings{\r\n        margin: 0;\r\n        padding: 0%;\r\n        height: 100%;\r\n        width: 100%;\r\n    \r\n        display: flex;\r\n        justify-content: center;\r\n        align-items: center;\r\n        padding: 9.5em;\r\n    }\r\n\r\n    .greetings h1{\r\n        font-size: 50px;\r\n        color: #283747;\r\n    }\r\n\r\n    .greetings span{\r\n        color: crimson;\r\n        font-weight: bold;\r\n    }\r\n\r\n    .container-grid {\r\n        display: grid;\r\n        grid: repeat(2, 1fr);\r\n    }\r\n\r\n    .btn .show:active {\r\n        background: rgb(247, 63, 247);\r\n    }\r\n</style>"]}]}